%% \pxhy@revoke@pdf@tc
\@onlypreamble\pxhy@revoke@pdf@tc
\def\pxhy@revoke@pdf@tc#1{%
  \expandafter\let\csname\pxhy@pdfenc\string#1\endcsname\@undefined
  \pxhy@make@txtcmd\pxhy@tmpa{\pxhy@pdfenc}#1%
  \ifx#1\pxhy@tmpa
    \pxhy@make@txtcmd#1?#1%
  \fi
}
